
// 定时任务
    ScheduleUtils.addCustomerTask(this.scheduleHost
        , invokeTime
        , ScheduleUtils.TASH_TYPE_AUTO_AUDIT
        , String.valueOf(recomId)
        ,this.pharmacistHost + "/inner/recommend/auto/audit"
        , paramMap
    );

